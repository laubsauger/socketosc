{
  "name": "socket-server",
  "version": "0.0.20",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "description": "socketosc server wrapped in electron",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "release": "electron-builder --mac --windows --linux --publish always",
    "start": "electron-forge start",
    "build": "cross-env CI=false NODE_ENV=production react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently --kill-others \"cross-env BROWSER=none yarn start:app\" \"wait-on http://localhost:3000 && electron .\"",
    "start:app": "react-scripts start",
    "start:server": "tsc && cross-env NODE_ENV=production node ./dist/server.js",
    "dev:server": "nodemon -e ts  --exec \"tsc && cross-env NODE_ENV=development node ./dist/server.js\"",
    "dev:serverdist": "cross-env NODE_ENV=development node ./dist/server.js",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "keywords": [],
  "author": "laubsauger",
  "license": "ISC",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/react-bootstrap": "^0.32.29",
    "bootstrap": "^5.1.3",
    "bootstrap-dark-5": "^1.1.3",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "https": "^1.0.0",
    "mobx": "^6.3.13",
    "mobx-react-lite": "^3.2.3",
    "osc": "^2.4.2",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "sass": "^1.49.7",
    "socket.io": "^4.4.0",
    "socket.io-client": "^4.4.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.66",
    "@electron-forge/maker-deb": "^6.0.0-beta.66",
    "@electron-forge/maker-rpm": "^6.0.0-beta.66",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.66",
    "@electron-forge/maker-zip": "^6.0.0-beta.66",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.14",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "concurrently": "^7.4.0",
    "electron": "^20.1.4",
    "electron-builder": "^23.3.3",
    "nodemon": "^2.0.15",
    "typescript": "^4.5.5",
    "wait-on": "^6.0.1"
  },
  "build": {
    "win": {
      "publish": [
        "github"
      ]
    },
    "mac": {
      "publish": [
        "github"
      ]
    },
    "linux": {
      "publish": [
        "github"
      ]
    }
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "socket_server"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
