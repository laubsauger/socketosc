{
  "name": "socket-server",
  "version": "0.0.31",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "description": "socketosc server wrapped in electron",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "release": "electron-builder --mac --windows --linux --publish always",
    "start": "electron-forge start",
    "build": "cross-env CI=false NODE_ENV=production react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently --kill-others \"cross-env BROWSER=none yarn start:app\" \"wait-on http://localhost:3000 && electron .\"",
    "start:app": "react-scripts start",
    "start:server": "tsc && cross-env NODE_ENV=production node ./dist/server.js",
    "dev:server": "nodemon -e ts  --exec \"tsc && cross-env NODE_ENV=development node ./dist/server.js\"",
    "dev:serverdist": "cross-env NODE_ENV=development node ./dist/server.js",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "keywords": [],
  "author": "laubsauger",
  "license": "ISC",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react-bootstrap": "^0.32.29",
    "bootstrap": "^5.1.3",
    "bootstrap-dark-5": "^1.1.3",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^3.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "https": "^1.0.0",
    "mobx": "^6.3.13",
    "mobx-react-lite": "^4.0.5",
    "osc": "^2.4.2",
    "query-string": "^8.2.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.1.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.1",
    "sass": "^1.49.7",
    "socket.io": "^4.4.0",
    "socket.io-client": "^4.4.0",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "concurrently": "^8.2.2",
    "electron": "^28.2.2",
    "electron-builder": "^24.9.1",
    "nodemon": "^3.0.3",
    "typescript": "^5.3.3",
    "wait-on": "^7.2.0"
  },
  "build": {
    "win": {
      "publish": [
        "github"
      ]
    },
    "mac": {
      "publish": [
        "github"
      ]
    },
    "linux": {
      "publish": [
        "github"
      ]
    }
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "socket_server"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
